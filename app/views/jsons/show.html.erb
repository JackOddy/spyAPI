<% if @api.jsons.any? %>

  <p>API url: https://spy-api.herokuapp.com/apis?api-key=<%=@api.key%>&json=<%=@json.name%></p>
  <br>
    name: <%= @json.name %>
    <%= link_to 'Edit JSON', edit_api_json_path(@api, @json) %>
    <%= link_to 'Delete JSON', api_json_path(@api,@json), method: :delete %>
    content: <%= @json.content %>
<% end %>
