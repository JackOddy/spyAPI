      <div class="api-title">
        <div class="api-key"> <div id='key'>key</div> <%= @api.key %> </div>
      </div>
      <% if @api.jsons.any? %>
      <div class="jsons-list">
        <% @api.jsons.each do |json| %>
        <div class="api">
          <div class='json-name'>
            <%= link_to json.name, api_json_path(@api,json) %>
          </div>
        </div>

        <% end %>
        <% else %>
        No JSONs yet
      <% end %>
      <br>
      <%= link_to 'Edit API ', edit_api_path(@api) %> |
      <%= link_to ' Delete API', api_path(@api), method: :delete %>
    </div>
